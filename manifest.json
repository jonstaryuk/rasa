{
   "manifest_version": 2,

   "name": "Rasa",
   "short_name": "Rasa",
   "description": "A minimal, fast-loading new tab page with optional clock and background colors.",
   "version": "1.3.0",

   "icons": {
      "48": "icon_48.png",
      "128": "icon_128.png"
   },

   "permissions": ["storage"],

   "chrome_url_overrides": {
      "newtab": "new_tab.html"
   },

   "options_ui": {
      "page": "options.html",
      "chrome_style": true
   }
}
